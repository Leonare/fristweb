[Rainmeter]
Update=1000

[Metadata]
Name=Gemini : Pollux Sidebar
Author=TruCola
Information=Pollux - Gaming / Entertainment sidebar with system monitoring.
Version=1.0

[Variables]
@include=#@#\Variables.inc
@include2=#@#\PolluxVariables.inc


:x----- Measure : Section 1 : User & Welcome -----x:
[MeasureTimeOfDay]
Measure=Time
Format="%H"
Substitute=#TimeOfDay#

[MeasureGameQuotes]
Measure=Plugin
Plugin=Plugins\QuotePlugin.dll
PathName=#@#\Text\pollux.txt
Subfolders=0
UpdateDivider=-1

[MeasureWeather]
Measure=Plugin
Plugin=WebParser.dll
UpdateRate=900
Url=http://wxdata.weather.com/wxdata/weather/local/#Location#?cc=*&unit=#Unit#
RegExp="(?siU)<head>.*<ut>(.*)</ut>.*<tmp>(.*)</tmp>.*<t>(.*)</t>"

[MeasureTempUnit]
Measure=Plugin
Plugin=WebParser.dll
Url=[MeasureWeather]
StringIndex=1

[MeasureWeatherTemp]
Measure=Plugin
Plugin=WebParser.dll
Url=[MeasureWeather]
StringIndex=2

[MeasureWeatherCond]
Measure=Plugin
Plugin=WebParser.dll
Url=[MeasureWeather]
StringIndex=3

:x----- Measure : Section 3 : System Stats -----x:
[MeasureHost]
